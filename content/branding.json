      "numbers_data": "Source Sans Pro Semi-Bold"
    },
    "css_font_mapping": {
      "source_sans_pro": {
        "description": "Headers, titles, navigation, buttons, form elements",
        "font_family": "'Source Sans Pro', sans-serif",
        "html_elements": [
          "h1", "h2", "h3", "h4", "h5", "h6",
          ".nav-links a", ".cta-button", ".secondary-button",
          ".form-group input", ".form-group select", ".form-group textarea",
          ".form-group label", ".logo", ".demo-link"
        ],
        "css_classes": [
          ".font-heading", ".font-title", ".font-nav", ".font-button", ".font-form"
        ]
      },
      "merriweather": {
        "description": "Body text, paragraphs, general content",
        "font_family": "'Merriweather', serif",
        "html_elements": [
          "body", "p", "li", "span", "div"
        ],
        "css_classes": [
          ".font-body", ".font-content", ".font-text"
        ]
      }
    }
