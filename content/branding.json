  "brand_colors": {
    "primary": "#2c5282",
    "accent": "#2d7d7d",
    "white": "#ffffff",
    "warm_gray": "#718096",
    "light_gray": "rgba(113, 128, 150, 0.1)"
  },
  "css_color_mapping": {
    "primary_navy": {
      "description": "Primary brand color for headers, main text, and important elements",
      "color_value": "var(--navy)",
      "html_elements": [
        "h1", "h2", "h3", "h4", "h5", "h6",
        ".nav-links a", ".logo"
      ],
      "css_classes": [
        ".color-primary", ".color-navy", ".color-heading", ".color-main"
      ]
    },
    "accent_teal": {
      "description": "Accent color for links, buttons, highlights, and interactive elements",
      "color_value": "var(--teal)",
      "html_elements": [
        ".nav-links a:hover", ".cta-button", ".demo-link",
        ".form-group label", ".contact-method"
      ],
      "css_classes": [
        ".color-accent", ".color-teal", ".color-link", ".color-highlight"
      ]
    },
    "warm_gray": {
      "description": "Secondary text color for body content and less prominent elements",
      "color_value": "var(--warm-gray)",
      "html_elements": [
        "p", "li", "span", ".form-note", ".contact-info"
      ],
      "css_classes": [
        ".color-secondary", ".color-gray", ".color-text", ".color-body"
      ]
    },
    "white": {
      "description": "White color for backgrounds, contrast text, and light elements",
      "color_value": "var(--white)",
      "html_elements": [
        ".cta-button", ".hero", ".header"
      ],
      "css_classes": [
        ".color-white", ".color-light", ".color-bg"
      ]
    },
    "light_gray": {
      "description": "Light gray for subtle backgrounds, borders, and dividers",
      "color_value": "var(--light-gray)",
      "html_elements": [
        ".header", ".form-group input", ".pilot-form"
      ],
      "css_classes": [
        ".color-light-gray", ".color-border", ".color-subtle"
      ]
    }
  },
  "css_background_mapping": {
    "hero_section": {
      "description": "Main hero section background - light gradient",
      "background_value": "linear-gradient(135deg, var(--white) 0%, #f8fafc 50%, #e2e8f0 100%)",
      "html_elements": [
        ".hero"
      ],
      "css_classes": [
        ".bg-hero", ".bg-light-gradient", ".bg-main"
      ]
    },
    "header_background": {
      "description": "Header background - semi-transparent white with blur",
      "background_value": "rgba(255, 255, 255, 0.95)",
      "html_elements": [
        ".header"
      ],
      "css_classes": [
        ".bg-header", ".bg-transparent-white", ".bg-blur"
      ]
    },
    "primary_gradient": {
      "description": "Primary brand gradient - navy to teal",
      "background_value": "linear-gradient(135deg, var(--navy) 0%, var(--teal) 100%)",
      "html_elements": [
        ".cta-button", ".section-dark", ".footer"
      ],
      "css_classes": [
        ".bg-primary-gradient", ".bg-brand", ".bg-dark"
      ]
    },
    "accent_gradient": {
      "description": "Accent gradient - teal to navy",
      "background_value": "linear-gradient(135deg, var(--teal) 0%, var(--navy) 100%)",
      "html_elements": [
        ".demo-link", ".contact-method", ".highlight-section"
      ],
      "css_classes": [
        ".bg-accent-gradient", ".bg-teal-navy", ".bg-highlight"
      ]
    },
    "teal_solid": {
      "description": "Solid teal background for buttons and highlights",
      "background_value": "var(--teal)",
      "html_elements": [
        ".demo-link:hover", ".contact-method:hover", ".button-teal"
      ],
      "css_classes": [
        ".bg-teal", ".bg-accent", ".bg-solid-teal"
      ]
    },
    "white_solid": {
      "description": "Solid white background for content sections",
      "background_value": "var(--white)",
      "html_elements": [
        ".content-section", ".form-section", ".light-section"
      ],
      "css_classes": [
        ".bg-white", ".bg-light", ".bg-content"
      ]
    },
    "light_gray_solid": {
      "description": "Light gray background for subtle sections",
      "background_value": "var(--light-gray)",
      "html_elements": [
        ".form-group input", ".pilot-form", ".subtle-section"
      ],
      "css_classes": [
        ".bg-light-gray", ".bg-subtle", ".bg-form"
      ]
    },
    "dark_gradient": {
      "description": "Dark gradient for footer and dark sections",
      "background_value": "linear-gradient(135deg, var(--navy) 0%, #1e293b 100%)",
      "html_elements": [
        ".footer", ".dark-section", ".section-dark"
      ],
      "css_classes": [
        ".bg-dark-gradient", ".bg-dark", ".bg-footer"
      ]
    },
    "warm_gray_gradient": {
      "description": "Warm gray gradient for neutral sections",
      "background_value": "linear-gradient(135deg, var(--warm-gray) 0%, #95a5a6 100%)",
      "html_elements": [
        ".neutral-section", ".gray-section"
      ],
      "css_classes": [
        ".bg-warm-gray-gradient", ".bg-neutral", ".bg-gray"
      ]
    }
  },
  "typography": {
    "google_fonts_href": "https://fonts.googleapis.com/css2?family=Source+Sans+Pro:ital,wght@0,300;0,400;0,600;0,700;1,400&family=Merriweather:ital,wght@0,300;0,400;0,700;1,300;1,400&display=swap",
    "heading_font": "Source Sans Pro",
