  "brand_foundation": {
    "mission": "Deliver immediate, measurable business value through rigorous, agentic AI solutions that orchestrate every aspect of clinical trials for biotech and pharma innovators.",
    "vision": "To be the AI-powered operating system, trusted by life sciences organizations to navigate and optimize clinical trials from start to finish.",
    "brand_promise": "Where Rigor Meets Orchestration.",
    "core_values": [
      "Rigor: Scientific integrity, validated methodologies",
      "Results-Driven: Measurable business impact",
      "Expert: Clinical and operational leadership",
      "Reliable: Trusted delivery across the trial lifecycle",
      "Transparent: Clear, honest communications"
    ]
  },
  "brand_voice": {
    "archetype_primary": "The Sage (wisdom, knowledge, trusted advisor)",
    "archetype_secondary": "The Navigator/Guide (direction, orchestration)",
    "voice_characteristics": [
      "Authoritative but approachable—expert guidance without arrogance",
      "Technical precision, accessible language (no jargon for jargon's sake)",
      "Evidence-based and insightful; every claim rooted in data",
      "Action-oriented, practical, and professional",
      "Confidence balanced by transparency—especially about development stage"
